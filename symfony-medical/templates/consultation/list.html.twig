{% block list_block %}
    <table>
        
        {% for a in list %}
        <tr>
            <td>{{ a.matricule }}</td>
            <td>{{ a.numSS }}</td>
            <td>{{ a.dateHeure | date('d/m/Y h:m')}}</td>
        </tr>
        {% else %}<tr>
            <td>Aucun {{ label }} n'a été trouvé.</td>
        </tr>
        {% endfor %}
    </table>
{% endblock %}